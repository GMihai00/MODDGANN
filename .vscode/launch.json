{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Python Script with Args",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/model/main.py",
            "args": [
                "--input_data",
                "${workspaceFolder}/src/dataset_helpers/data.csv",
                "--model_name",
                "AlteredInceptionV3",
                "--epochs",
                "40",
                "--model_type",
                "ensemble",
                "--number_folds",
                "5",
                "--learning_rate",
                "0.0008",
                "--batch_size",
                "16"
            ],
            "console": "integratedTerminal"
        }
    ]
}